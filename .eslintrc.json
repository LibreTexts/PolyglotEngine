{
  "root": true,
  "env": {
      "node": true,
      "es2021": true
  },
  "extends": [
      "airbnb",
      "plugin:jsdoc/recommended"
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "jsdoc"
  ],
  "rules": {
      "no-console": "off",
      "import/extensions": "off"
  }
}